<h1>Genes#index</h1>
<p>Find me in app/views/genes/index.html.erb</p>


<table>
  <tr>
    <th> Gene name </th>
    <th> Gene symbol </th>
  </tr>
  <% @genes.each do |g| %>
  <tr>
    <td><%= g.name %></td>
    <td><%= g.symbol %></td>
  </tr>
  <% end %>
</table>

  <a><%= link_to "New Gene!", new_gene_path %></a>
